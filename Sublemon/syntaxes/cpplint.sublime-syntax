---
name: Cpplint Output
scope: text.log.cpplint
hidden: true

contexts:
  main:
    - match: '^(.+?):(\d+): .+? (\[[^\]]+\]) (\[[^\]]+\])$'
      captures:
        1: entity.name.filename
        2: constant.numeric
        3: style.italic comment.category
        4: style.italic comment.confidense
