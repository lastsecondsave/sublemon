---
name: Cpplint Output
scope: text.output.cpplint
hidden: true

contexts:
  main:
    - match: '^(.+?):(\d+): .+? (\[[^\]]+\]) (\[[^\]]+\])$'
      captures:
        1: entity.name.filename
        2: constant.numeric.line
        3: comment.category.description
        4: comment.category.confidense
