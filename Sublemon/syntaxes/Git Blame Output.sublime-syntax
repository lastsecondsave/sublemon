%YAML 1.2
---
name: Git Blame Output
scope: text.output.git.blame
hidden: true

contexts:
  main:
    - match: ^(.+?)  (.+?) ([0-9a-f]+)\s+(\d+)
      captures:
        1: constant.numeric.date.git
        2: entity.name.author.git
        3: storage.category.hash.git
        4: constant.numeric.line-number.git

    - match: ^\s+(\d+)
      captures:
        1: constant.numeric.line-number.git
